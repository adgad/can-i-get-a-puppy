{
    "name": "Can I Get A Puppy",
    "version": "1.1",
    "description": "Demand a puppy!",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "index.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "*://*.ft.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.ft.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}
